# Power saving profile
on property:fku.perf.profile=0
    write /sys/devices/system/cpu/cpu0/cpufreq/scaling_min_freq 300000
    write /sys/devices/system/cpu/cpu0/cpufreq/scaling_max_freq 1248000
    write /sys/devices/system/cpu/cpu4/cpufreq/scaling_max_freq 902400
    write /sys/devices/system/cpu/cpu4/cpufreq/interactive/hispeed_freq 652800
    write /sys/module/cpu_input_boost/parameters/input_boost_freq_lp 748800
    write /sys/module/cpu_input_boost/parameters/input_boost_freq_hp 0
    write /sys/module/cpu_input_boost/parameters/input_boost_duration 64
    write /sys/class/kgsl/kgsl-3d0/max_pwrlevel 4

# Balance (FK defaults)
on property:fku.perf.profile=1
    write /sys/devices/system/cpu/cpu0/cpufreq/scaling_min_freq 300000
    write /sys/devices/system/cpu/cpu0/cpufreq/scaling_max_freq 1900800
    write /sys/devices/system/cpu/cpu4/cpufreq/scaling_max_freq 2112000
    write /sys/devices/system/cpu/cpu4/cpufreq/interactive/hispeed_freq 1574400
    write /sys/module/cpu_input_boost/parameters/input_boost_freq_lp 960000
    write /sys/module/cpu_input_boost/parameters/input_boost_freq_hp 652800
    write /sys/module/cpu_input_boost/parameters/input_boost_duration 100
    write /sys/class/kgsl/kgsl-3d0/max_pwrlevel 0

# Performance
on property:fku.perf.profile=2
    write /sys/devices/system/cpu/cpu0/cpufreq/scaling_min_freq 672000
    write /sys/devices/system/cpu/cpu0/cpufreq/scaling_max_freq 1900800
    write /sys/devices/system/cpu/cpu4/cpufreq/scaling_max_freq 2457600
    write /sys/devices/system/cpu/cpu4/cpufreq/interactive/hispeed_freq 1747200
    write /sys/module/cpu_input_boost/parameters/input_boost_freq_lp 1555200
    write /sys/module/cpu_input_boost/parameters/input_boost_freq_hp 1267200
    write /sys/module/cpu_input_boost/parameters/input_boost_duration 125
    write /sys/class/kgsl/kgsl-3d0/max_pwrlevel 0
