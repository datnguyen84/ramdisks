# Power saving profile
on property:fku.perf.profile=0
    write /sys/devices/system/cpu/cpu0/cpufreq/scaling_min_freq 307200
    write /sys/devices/system/cpu/cpu0/cpufreq/scaling_max_freq 1286400
    write /sys/devices/system/cpu/cpu2/cpufreq/scaling_max_freq 748800
    write /sys/devices/system/cpu/cpu2/cpufreq/interactive/hispeed_freq 537600
    write /sys/module/cpu_boost/parameters/input_boost_freq "0:748800 2:748800"
    write /sys/module/cpu_boost/parameters/input_boost_ms 150
    write /sys/class/kgsl/kgsl-3d0/devfreq/governor powersave
    write /sys/class/leds/lcd-backlight/max_brightness 100

# Balance (FK defaults)
on property:fku.perf.profile=1
    write /sys/devices/system/cpu/cpu0/cpufreq/scaling_min_freq 307200
    write /sys/devices/system/cpu/cpu0/cpufreq/scaling_max_freq 1593600
    write /sys/devices/system/cpu/cpu2/cpufreq/scaling_max_freq 2342400
    write /sys/devices/system/cpu/cpu2/cpufreq/interactive/hispeed_freq 825600
    write /sys/module/cpu_boost/parameters/input_boost_freq "0:1209600 2:825600"
    write /sys/module/cpu_boost/parameters/input_boost_ms 40
    write /sys/class/kgsl/kgsl-3d0/devfreq/governor msm-adreno-tz
    write /sys/class/leds/lcd-backlight/max_brightness 255

# Performance
on property:fku.perf.profile=2
    write /sys/devices/system/cpu/cpu0/cpufreq/scaling_min_freq 691200
    write /sys/devices/system/cpu/cpu0/cpufreq/scaling_max_freq 1593600
    write /sys/devices/system/cpu/cpu2/cpufreq/scaling_max_freq 2342400
    write /sys/devices/system/cpu/cpu2/cpufreq/interactive/hispeed_freq 1747200
    write /sys/module/cpu_boost/parameters/input_boost_freq "0:1593600 2:1593600"
    write /sys/module/cpu_boost/parameters/input_boost_ms 1500
    write /sys/class/kgsl/kgsl-3d0/devfreq/governor performance
    write /sys/class/leds/lcd-backlight/max_brightness 255
